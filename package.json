{
  "name": "jupyterlab-plugin-graph",
  "version": "0.1.2",
  "description": "JupyterLab extension to show an interactive dependency graph of the installed plugins",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jtpio/jupyterlab-plugin-graph",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlab-plugin-graph/issues"
  },
  "license": "BSD-3-Clause",
  "author": "jupyterlab-plugin-graph contributors",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlab-plugin-graph.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/ui-components": "^2.2.0",
    "@lumino/signaling": "^1.4.2",
    "@lumino/widgets": "^1.13.2",
    "cytoscape": "^3.14.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.8.8",
    "@types/react": "~16.9.16",
    "@types/react-dom": "~16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "~3.9.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
